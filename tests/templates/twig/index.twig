    <div class="drawer-cover"></div>
    {% if cart_model %}
    <div class="drawer critical-hide" tabindex="-1" data-section-id="cart-drawer" data-section-type="cart-drawer" data-cart-token="" data-drawer-push aria-hidden="true">
        <form action="{{ home_url }}/checkout" method="post" novalidate class="cart-drawer{{ cart.item_count>0?'':' cart-drawer--empty' }}">
            <div class="cart-drawer__header">
                <div class="cart-drawer__header-container">
                    <button type="button" class="cart-drawer__close-button btn btn--clear btn--square">
                        <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-close" viewBox="0 0 16.8 16.8">
                            <path fill="#444" d="M16.8 1.5L15.4.1 8.4 7l-7-7L0 1.4l7 7-7 7 1.4 1.4 7-7 7 7 1.4-1.4-7-7z"/>
                        </svg>
                        <span class="icon__fallback-text">close</span>
                    </button>
                    <p class="cart-drawer__header-title text-title text-center">{{ lang.shopping_cart_title }}</p>
                </div>
            </div>
            <div class="cart-drawer__content">
                <div class="cart-drawer__content-container">
                    <div class="cart-drawer__empty-text">
                        <p class="text-center">{{ lang.shopping_cart_empty }}</p>
                    </div>
                    <div class="cart-drawer__item-list" data-cart-item-list>
                        {% if cart.item_count >0  %}
                        {% for item in cart.items  %}
                        <div class="cart-drawer__item" data-cart-item data-cart-item-id="{{ item.key }}" aria-live="polite">
                            <a href="{{ item.url }}" class="cart-drawer__item-image" style="background-image: url('{{ item.image }}');" data-cart-item-background-image data-cart-item-href></a>
                            <div class="cart-drawer__item-content">
                                <a href="{{ item['url'] }}" class="h4 cart-drawer__item-title" data-cart-item-href data-cart-item-title>{{ item['title'] }}</a>
                                <p class="cart-drawer__item-subtitle" data-cart-item-variant-title>{{ item['attribute'] }}</p>
                                <ul class="cart-drawer__item-properties" data-cart-item-property-list>
                                    <li class="cart-drawer__item-property ajax-cart__template" data-cart-item-property>t: e
                                    </li>
                                </ul>
                                <p class="cart-drawer__item-price-container" data-cart-item-price-container>
                                    <span class="cart-item__original-price cart-item__price">{{ currency_prefix==1?currency_code:'' }}{{ item.price }}{{ currency_prefix!=1?currency_code:'' }}</span>
                                </p>
                                <ul class="order-discount order-discount--list order-discount--title order-discount--cart" aria-label="ト" data-cart-item-discount-list></ul>
                                <button class="cart-drawer__item-delete btn-link" type="button" data-cart-item-delete>{{ lang.delete }}</button>
                                <label for="quantity_" class="label--hidden" data-cart-item-label-quantity>{{ lang.piece }}</label>
                                <input id="quantity_" class="cart-drawer__item-quantity" value="{{ item.quantity }}" type="number" pattern="\d*" min="1" aria-label="{{ lang.piece }}" data-cart-item-input-quantity>
                            </div>
                            <p class="cart-item__message text-center" aria-hidden="true" data-item-message></p>
                        </div>
                        {% endfor %}
                        {% else %}
                        <div class="cart-drawer__item ajax-cart__template" data-cart-item data-cart-item-id="" aria-live="polite">
                            <a href="" class="cart-drawer__item-image" style="background-image: none;" data-cart-item-background-image data-cart-item-href></a>
                            <div class="cart-drawer__item-content">
                                <a href="" class="h4 cart-drawer__item-title" data-cart-item-href data-cart-item-title></a>
                                <p class="cart-drawer__item-subtitle" data-cart-item-variant-title></p>
                                <ul class="cart-drawer__item-properties" data-cart-item-property-list>
                                    <li class="cart-drawer__item-property ajax-cart__template" data-cart-item-property>t: e
                                    </li>
                                </ul>
                                <p class="cart-drawer__item-price-container" data-cart-item-price-container>
                                    <span class="cart-item__original-price cart-item__price"></span>
                                </p>
                                <ul class="order-discount order-discount--list order-discount--title order-discount--cart" aria-label="" data-cart-item-discount-list>
                                    <li class="order-discount__item ajax-cart__template" data-cart-item-discount>
                                        <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-saletag">
                                            <path d="M10 3a1 1 0 1 1 0-2 1 1 0 0 1 0 2zm0-3H7a1 1 0 0 0-.71.29l-6 6a1 1 0 0 0 0 1.42l4 4a1 1 0 0 0 1.42 0c.19-.2 5.8-5.81 6-6A1 1 0 0 0 12 5V2a2 2 0 0 0-2-2z" fill="#231F20"/>
                                        </svg>
                                        <span data-cart-item-discount-title></span>
                                        (-<span data-cart-item-discount-amount></span>
                                        )
                
                                    </li>
                                </ul>
                                <button class="cart-drawer__item-delete btn-link" type="button" data-cart-item-delete>{{ lang.delete }}</button>
                                <label for="quantity_" class="label--hidden" data-cart-item-label-quantity>{{ lang.piece }}</label>
                                <input id="quantity_" class="cart-drawer__item-quantity" value="" type="number" pattern="\d*" min="1" aria-label="{{ lang.piece }}" data-cart-item-input-quantity>
                            </div>
                            <p class="cart-item__message text-center" aria-hidden="true" data-item-message></p>
                        </div>
                        {% endif  %}
                    </div>
                </div>
            </div>
            <div class="cart-drawer__footer">
                <div class="cart-drawer__footer-container">
                    <div class="cart-drawer__message-container">
                        <div class="cart-drawer__message" data-cart-message></div>
                    </div>
                    <div data-cart-discount-container>
                        <div class="order-discount-cart-drawer-wrapper ajax-cart__template" data-cart-discount>
                            <span class="order-discount order-discount--title order-discount--cart-title">
                                <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-saletag">
                                    <path d="M10 3a1 1 0 1 1 0-2 1 1 0 0 1 0 2zm0-3H7a1 1 0 0 0-.71.29l-6 6a1 1 0 0 0 0 1.42l4 4a1 1 0 0 0 1.42 0c.19-.2 5.8-5.81 6-6A1 1 0 0 0 12 5V2a2 2 0 0 0-2-2z" fill="#231F20"/>
                                </svg>
                                <span class="visually-hidden">ディスカウント:</span>
                                <span data-cart-discount-title></span>
                            </span>
                            <span class="order-discount order-discount--cart-price">
                                -<span data-cart-discount-amount></span>
                            </span>
                        </div>
                    </div>
                    <div class="cart-drawer__subtotal" aria-live="polite">
                        <div class="cart-drawer__subtotal-label">
                            <span class="h4">{{ lang.check_total }}</span>
                        </div>
                        <div class="cart-drawer__subtotal-price">
                            <span class="cart-drawer__subtotal-number" data-cart-subtotal>{{ currency_prefix==1?currency_code:'' }}{{ cart.total_price }}{{ currency_prefix!=1?currency_code:'' }}</span>
                        </div>
                    </div>
                    <script>
                    function openCheckout(url){
                        fbq('track', 'InitiateCheckout');
                        setTimeout(()=>{
                            window.location.href = url;
                        },250)
                        
                    }
                    </script>
                    <a onclick="openCheckout('{{ home_url }}/checkout')" class="btn btn--loader cart-drawer__checkout" name="checkout" data-cart-submit>
                        {{ lang.now_buy }}
      <span class="btn--loader-balls"></span>
                    </a>
                </div>
            </div>
        </form>
    </div>
    {% endif %}
    <a href="#" title="Top" class="back-to-top">
        <span></span>
        <div>
            <i class="icon123123 "></i>
        </div>
    </a>
    <style>
        .icon123123 {
            background-image: url("https://cdn.shopify.com/s/files/1/0429/1507/0101/files/lv-top.jpg?v=1594276697");
            background-size: 40px 40px;
            z-index: 99;
        }

        .back-to-top {
            position: fixed;
            bottom: 7em;
            right: 12px;
            text-decoration: none;
            color: #33323200;
            background-color: #eee;
            font-size: 20px;
            padding: 0em;
            display: none;
            -webkit-border-top-left-radius: 1px;
            -webkit-border-bottom-left-radius: 1px;
            -webkit-border-top-right-radius: 1px;
            -webkit-border-bottom-right-radius: 1px;
            -moz-border-radius-topleft: 1px;
            -moz-border-radius-bottomleft: 1px;
            -moz-border-radius-topright: 1px;
            -moz-border-radius-bottomright: 1px;
            border-top-left-radius: 100px;
            border-bottom-left-radius: 100px;
            border-top-right-radius: 100px;
            border-bottom-right-radius: 100px;
            z-index: 60000;
        }

        .back-to-top i {
            vertical-align: middle;
            position: fixed;
            bottom: 7em;
            right: 0.6em;
            z-index: 100000;
            width: 40px;
            height: 40px;
        }

        .back-to-top span {
            padding-left: 0;
        }

        .back-to-top i + span {
            padding-left: 0;
        }

        .back-to-top:hover {
            text-decoration: none;
            color: #555;
        }
    </style>
</body>
</html>
